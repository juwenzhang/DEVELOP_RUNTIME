{
  "name": "@develop_runtime/node-ast",
  "version": "1.0.0",
  "description": "Node.js AST processing library",
  "displayName": "node-ast",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "lint:syntax": "pnpm exec tsc --noEmit",
    "build:native": "pnpm exec node-gyp configure build",
    "build:ts": "pnpm exec tsc",
    "build": "pnpm run build:native && pnpm run build:ts",
    "help": "pnpm exec node-gyp help"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/node": "^24.7.0",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^11.4.2",
    "typescript": "^5.9.3"
  }
}
